{
  "name": "browser-id3-writer",
  "version": "2.3.0",
  "description": "Browser & Node.js ID3 tag writer in pure JS",
  "main": "dist/browser-id3-writer.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build": "webpack",
    "test": "npm run lint && npm run build && npm run test:browser && npm run test:node",
    "test:browser": "karma start --single-run",
    "test:node": "mocha -c test/node.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egoroof/browser-id3-writer.git"
  },
  "keywords": [
    "browser",
    "nodejs",
    "writer",
    "id3",
    "mp3",
    "audio"
  ],
  "author": "egoroof",
  "files": [
    "LICENSE.md",
    "README.md",
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/egoroof/browser-id3-writer/issues"
  },
  "homepage": "https://github.com/egoroof/browser-id3-writer#readme",
  "devDependencies": {
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "chai": "3.5.0",
    "eslint": "2.8.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-firefox-launcher": "0.1.7",
    "karma-ie-launcher": "0.2.0",
    "karma-mocha": "0.2.2",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "webpack": "1.13.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
